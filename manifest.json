{
    "name": "Deep Focus",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": ["storage"],
    "background": {
        "scripts": ["background.js","script.js"],
        "persistent": false
    },
    "content_scripts": [{
        "js": ["jquery-3.6.0.min.js","script.js"],
        "matches": ["http://*/*", "https://*/*"]
      }],
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/16.png",
            "32": "images/32.png",
            "48": "images/48.png",
            "128": "images/128.png"
        }
    },
    "icons": {
        "16": "images/16.png",
        "32": "images/32.png",
        "48": "images/48.png",
        "128": "images/128.png"
      },
    "manifest_version": 2
  }