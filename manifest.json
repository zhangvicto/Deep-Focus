{
    "name": "Deep Focus",
    "version": "1.0",
    "description": "Focus Assist - No more distractions and get back to work!",
    "permissions": ["storage","activeTab"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "content_scripts": [{
        "js": ["content.js","script.js"],
        "matches": ["http://*/*", "https://*/*"]
      }],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/16.png",
            "32": "images/32.png",
            "48": "images/48.png",
            "128": "images/128.png"
        }
    },
    "icons": {
        "16": "images/16.png",
        "32": "images/32.png",
        "48": "images/48.png",
        "128": "images/128.png"
      },
    "manifest_version": 2
  }